    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "archiver": "^7.0.1",
    "axios": "^1.13.2",
    "bwip-js": "^4.8.0",
    "canvas": "^3.2.0",
    "dotenv": "^17.2.4",
    "elysia": "latest",
